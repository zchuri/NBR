Package: NBR
Type: Package
Title: Network-Based R-Statistics using Mixed Effects Models
Version: 0.1.4
Authors@R: c(
    person("Zeus", "Gracia-Tabuenca", email = "zgtabuenca@comunidad.unam.mx", role =  c("aut", "cre")),
